      "rationale": "モデル出力が無効なため要確認",
      "approval_token_pattern": "^[A-Za-z0-9._-]+$"
    },
    "rag_router": {
      "mode": "kedb_documents",
      "reason": "llm_failed_fallback_default",
      "query_strategy": "normalized_event_text",
      "filters": {
        "use_service_name": true,
        "use_ci_ref": true,
        "use_problem_number": true,
        "use_known_error_number": true
      }
    },
    "jobs_preview": {
      "invalid_output_next_action": "ask_clarification",
      "invalid_workflow_next_action": "ask_clarification",
      "missing_workflow_next_action": "ask_clarification",
      "default_confidence_on_invalid": 0.0,
      "rationale_on_invalid": "プレビュー出力が無効なため要確認",
      "rationale_on_invalid_workflow": "実行ワークフローがカタログに存在しないため要確認",
      "rationale_on_missing_workflow": "workflow_id を特定できないため要確認"
    },
    "routing_decide": {
      "needs_manual_triage": true,
      "confidence": 0,
      "reason_map": {
        "output_missing": "routing_decide_output_missing",
        "selected_policy_id_missing": "selected_policy_id_missing",
        "selected_policy_not_found": "selected_policy_not_found_in_candidates",
        "reply_target_missing": "reply_target_missing"
      }
    },
    "initial_reply": {
      "templates": {
        "auto_enqueue": "リクエストを受け付けました。処理は順番待ちに登録されました。\n\n概要: {summary}\n{job_line}",
        "require_approval": "承認が必要です。\n\n概要: {summary}\n有効期限: {expires_at}",
        "ask_clarification": "追加で確認したいことがあります。\n\n概要: {summary}",
        "reject": "この依頼は自動実行できません。\n\n概要: {summary}"
      },
      "default_template": "依頼内容を確認中です。\n\n概要: {summary}"
    },
    "feedback_decide": {
      "case_status": "open",
      "confidence": 0,
      "rationale": "モデル出力が無効なため安全側（open）にフォールバック"
    },
    "feedback_request_render": {
      "default_text": "feedback <job_id> <resolved:true|false> <score> <comment>",
      "clarifying_question": "job_id を教えてください"
    },
    "job_result_reply": {
      "success_template": "ジョブ {job_id} を完了しました。\n\n結果: {result}\n{trace_line}\n{feedback_request}",
      "failure_template": "ジョブ {job_id} が失敗しました。\n\nエラー: {error}\n{trace_line}\n{feedback_request}",
      "unknown_status_template": "ジョブ {job_id} の結果を受け取りました。\n\n結果: {result}\n{trace_line}\n{feedback_request}"
    }
  }
}
