        "query": "SELECT\n  context_id,\n  source,\n  status,\n  normalized_event->'enrichment_plan' AS enrichment_plan,\n  normalized_event->'enrichment_summary' AS enrichment_summary,\n  normalized_event->'enrichment_refs' AS enrichment_refs,\n  normalized_event->'enrichment_details' AS enrichment_details,\n  normalized_event->'enrichment_error' AS enrichment_error\nFROM aiops_context\nWHERE context_id = NULLIF(NULLIF('{{ $json.context_id }}',''), 'undefined')::uuid\nLIMIT 1;"
      },
